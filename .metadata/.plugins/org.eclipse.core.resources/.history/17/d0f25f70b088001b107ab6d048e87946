package com.ujiankedua;

public class UjianKedua {

	private int platfon;
	private double bunga;
	private int lamapinjaman;
	
	public int getPlatfon() {
		return platfon;
	}

	public void setPlatfon(int platfon) {
		this.platfon = platfon;
	}

	public double getBunga() {
		return bunga;
	}

	public void setBunga(double bunga) {
		this.bunga = bunga;
	}

	public int getLamapinjaman() {
		return lamapinjaman;
	}

	public void setLamapinjaman(int lamapinjaman) {
		this.lamapinjaman = lamapinjaman;
	}

	
	
	
	public UjianKedua(int platfon, double bunga, int lamapinjaman) {
		this.platfon = platfon;
		this.bunga = bunga;
		this.lamapinjaman = lamapinjaman;
	}
	
	public UjianKedua() {
		
	}
}
